{
  "name": "azuretoolsforvscode",
  "displayName": "AzureToolsForVsCode",
  "description": "",
  "version": "0.0.1",
  "publisher": "microsoft",
  "repository": {
    "type": "git",
    "url": "https://github.com/bradygaster/azure-tools-vscode.git"
  },
  "engines": {
    "vscode": "^1.4.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:azure.logintoazure"
  ],
  "main": "./extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Azure Tools configuration",
      "properties": {
        "azure.tenantId": {
          "type": "string",
          "default": "guid",
          "description": "Your AAD Tenant ID from the portal. You need to set this if you intend on logging in using an MSA (@outlook.com, @live.com, @hotmail.com, etc.)."
        }
      }
    },
    "commands": [
      {
        "command": "azure.selectsubscription",
        "title": "azure subscriptions list"
      },
      {
        "command": "azure.logintoazure",
        "title": "azure login"
      },
      {
        "command": "azure.createwebapp.advanced",
        "title": "azure create webapp (advanced)"
      },
      {
        "command": "azure.createwebapp.simple",
        "title": "azure create webapp (simple)"
      },
      {
        "command": "azure.browseInPortal",
        "title": "azure browse resource in portal"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "adal-node": "^0.1.17",
    "async": "0.2.7",
    "azure": "1.2.0-preview",
    "azure-arm-authorization": "^3.1.1",
    "azure-arm-resource": "^1.4.4-preview",
    "azure-arm-website": "^0.11.4",
    "azure-asm-subscription": "^0.10.2",
    "copy-paste": "^1.3.0",
    "get-urls": "^5.0.1",
    "moment": "^2.14.1",
    "ms-rest": "^1.15.0",
    "ms-rest-azure": "~1.15.1",
    "open": "0.0.5",
    "uuid": "2.0.1"
  }
}